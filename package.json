{
  "name": "less-code-web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "spellcheck": "cspell lint --dot --gitignore --color --cache --show-suggestions \"src/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md|vue)\"",
    "type-check": "vue-tsc --build --force",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:style": "stylelint **/*.{vue,css}",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@guolao/vue-monaco-editor": "^1.5.1",
    "@icon-park/vue-next": "^1.4.2",
    "@tiptap/core": "^2.2.5",
    "@tiptap/pm": "^2.2.5",
    "@tiptap/starter-kit": "^2.2.5",
    "@tiptap/vue-3": "^2.2.5",
    "@visactor/vtable": "^0.23.1",
    "@vue-flow/additional-components": "^1.3.3",
    "@vue-flow/core": "^1.33.5",
    "@vueuse/core": "^10.9.0",
    "copy-text-to-clipboard": "^3.2.0",
    "d3": "^7.9.0",
    "echarts": "^5.5.0",
    "element-plus": "^2.6.3",
    "nanoid": "^5.0.7",
    "pinia": "^2.1.7",
    "rollup-plugin-visualizer": "^5.12.0",
    "smooth-dnd": "^0.12.1",
    "vee-validate": "^4.12.6",
    "vue": "^3.4.21",
    "vue-echarts": "^6.6.9",
    "vue-json-pretty": "^2.4.0",
    "vue-router": "^4.3.0",
    "zrender": "^5.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@commitlint/cz-commitlint": "17.6.7",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/codemirror": "^5.60.15",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "4.3.0",
    "cspell": "8.6.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "8.0.3",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.2",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-vue": "1.0.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vite-plugin-vue-devtools": "^7.0.18",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.6"
  }
}